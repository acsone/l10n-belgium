<odoo>
    <record id="l10n_be_annual_account_xbrl_multi_company_rule" model="ir.rule">
        <field name="name">l10n.be.annual.account.xbrl multi company</field>
        <field name="model_id" ref="model_l10n_be_annual_account_xbrl" />
        <field name="perm_read" eval="1" />
        <field name="perm_write" eval="1" />
        <field name="perm_create" eval="1" />
        <field name="perm_unlink" eval="1" />
        <field name="domain_force">
            ['|',('company_id','=',False),('company_id', 'in', company_ids)]
        </field>
    </record>
